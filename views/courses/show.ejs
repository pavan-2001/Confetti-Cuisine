<h2> Course data for <%=course.title %></h2>
<div class="table-wrapper">
    <table class="fl-table">
        <tr>
            <th>Title</th>
            <td><%=course.title%></td>
        </tr>
        <tr>
            <th>Description</th>
            <td><%=course.description%></td>
        </tr>
        <tr>
            <th>Max Students</th>
            <td><%=course.maxStudents%></td>
        </tr>
        <tr>
            <th>Cost</th>
            <td><%=course.cost%></td>
        </tr>
    </table>
</div>